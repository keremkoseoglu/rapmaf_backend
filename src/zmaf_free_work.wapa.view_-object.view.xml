<mvc:View                                                                                                                                                                                                                                                      
	controllerName="maffreeworklist.controller.Object"                                                                                                                                                                                                            
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:mvc="sap.ui.core.mvc"                                                                                                                                                                                                                                   
	xmlns:semantic="sap.f.semantic">                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
	<semantic:SemanticPage                                                                                                                                                                                                                                        
		id="page"                                                                                                                                                                                                                                                    
		headerPinnable="false"                                                                                                                                                                                                                                       
		toggleHeaderOnTitleClick="false"                                                                                                                                                                                                                             
		busy="{objectView>/busy}"                                                                                                                                                                                                                                    
		busyIndicatorDelay="{objectView>/delay}">                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
		<semantic:titleHeading>                                                                                                                                                                                                                                      
			<Title                                                                                                                                                                                                                                                      
				text="{MaterialDescription}"                                                                                                                                                                                                                               
				level="H2"/>                                                                                                                                                                                                                                               
		</semantic:titleHeading>                                                                                                                                                                                                                                     
                                                                                                                                                                                                                                                               
		<semantic:headerContent>                                                                                                                                                                                                                                     
			<ObjectNumber                                                                                                                                                                                                                                               
				number="{                                                                                                                                                                                                                                                  
					path: 'Height',                                                                                                                                                                                                                                           
					formatter: '.formatter.numberUnit'                                                                                                                                                                                                                        
				}"                                                                                                                                                                                                                                                         
				unit="{SizeUom}"                                                                                                                                                                                                                                           
			/>                                                                                                                                                                                                                                                          
			<ObjectAttribute id="MafId" title="Maf" text="{MafId}"></ObjectAttribute>                                                                                                                                                                                   
		</semantic:headerContent>                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
		<semantic:sendEmailAction>                                                                                                                                                                                                                                   
			<semantic:SendEmailAction id="shareEmail" press=".onShareEmailPress"/>                                                                                                                                                                                      
		</semantic:sendEmailAction>                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
		<semantic:titleCustomIconActions>                                                                                                                                                                                                                            
			<OverflowToolbarButton icon="sap-icon://accept" press=".onApprovePress"></OverflowToolbarButton>                                                                                                                                                            
			<OverflowToolbarButton icon="sap-icon://decline" press=".onRejectPress"></OverflowToolbarButton>                                                                                                                                                            
		</semantic:titleCustomIconActions>                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		<semantic:content>                                                                                                                                                                                                                                           
			<ObjectAttribute title="Created By" text="{CreatedBy}"></ObjectAttribute>                                                                                                                                                                                   
		</semantic:content>                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
	</semantic:SemanticPage>                                                                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
</mvc:View>                                                                                                                                                                                                                                                    